[
  <%- @availabilities.each_with_index do |availability, index| -%>
    {
        "id":         <%= index %>,
        "title":      "<%= availability[1] %>",
        "start":      "<%= availability[0].iso8601 %>",
        "end":        "<%= (availability[0] + 1.hour).iso8601 %>",
        "allDay":     false,
        <%- case availability[1]/@number_of_firemen.to_f -%>
        <%- when 0..0.25 -%>
          "className": "fc-event_red"
        <%- when 0.25..0.5 -%>
          "className": "fc-event_orange"
        <%- when 0.5..0.75 -%>
          "className": "fc-event_dark_green"
        <%- when 0.75..1 -%>
          "className": "fc-event_green"
        <%- end -%>
    }
    <%- if index != @availabilities.size-1 %>,<%- end -%>
  <%- end -%>
]
