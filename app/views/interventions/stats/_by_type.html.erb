<div class="row">
  <div class="col-md-9">
    <%= high_chart("chart", @chart) %>
  </div>
  <div class="col-md-3">
    <table class="table table-bordered table-striped">
      <thead>
        <tr>
          <th>Type</th>
          <th>Nombre</th>
        </tr>
      </thead>
      <tbody>
        <%- data.each.map do |kind, number| -%>
          <tr>
            <td><%= t("intervention.kind.#{Intervention::KIND.key(kind)}") %></td>
            <td><%= number %></td>
          </tr>
        <%- end -%>
      </tbody>
    </table>
  </div>
</div>
