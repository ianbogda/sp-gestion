<div class="row">
  <div class="col-md-6">
    <%= high_chart("chart", @chart) %>    
  </div>
  <div class="col-md-3">
    <table class="table table-bordered table-striped">
      <thead>
        <tr>
          <th>Mois</th>
          <th>Nombre</th>
        </tr>
      </thead>
      <tbody>
      <%- data.each_with_index do |number, i| -%>
        <tr>
          <td><%= t("date.month_names")[i+1].capitalize %></td>
          <td><%= number %></td>
        </tr>
      <%- end -%>
      </tbody>
    </table>
  </div>
</div>
