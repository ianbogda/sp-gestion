<div class="btn-toolbar pull-right">
  <div class="btn-group">
    <%= link_to(icon_label_text('icon-plus', 'Créer une nouvelle liste de matériel'),
                new_check_list_path,
                :class => 'btn btn-small') %>
  </div>
</div>
<table class="table table-bordered table-striped">
  <thead>
    <tr>
      <th>Titre</th>
      <th class="actions">Copier la liste</th>
    </tr>
  </thead>
  <tbody>
    <%= render(:partial => @check_lists) || raw("<tr><td colspan='2'>Pas encore de liste de matériel !</td></tr>") %>
  </tbody>
</table>
<%= page_navigation_links(@check_lists) %>