<div class="span-6 first">
  <p class="right"><%= link_to(image_tag("back/icons/add.png") + "&nbsp;Créer une nouvelle liste de matériel", new_check_list_path)%></p>
  <table id="check_lists">
    <thead>
      <tr>
        <th>Titre</th>
        <th class="actions">Copier la liste</th>
      </tr>
    </thead>
    <tbody>
      <%= render(:partial => @check_lists) || "<tr><td colspan='2'>Pas encore de liste de matériel !</td></tr>" %>
    </tbody>
  </table>
  <%= will_paginate(@check_lists, :previous_label => "", :next_label => "") %>
</div>